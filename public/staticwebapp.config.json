{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/favicon.ico"]
  },
  "responseOverrides": {
    "403": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["anonymous"]
    }
  ]
}

